@model List<Data.Models.Order>

@{
    ViewBag.Title = "Orders Index";
}

<h2>Orders Index</h2>

<table class="table">
    <tr>
        <th>Order ID</th>
        <th>Source Language</th>
        <th>Final File ID</th>
        <th>Status</th>
        <th>Order Date</th>
        <th>Order Cost</th>
        <th>Action</th> 
    </tr>
    @foreach (var order in Model)
    {
        <tr>
            <td>@order.OrderId</td>
            <td>@order.SourceLanguage</td>
            <td>@order.FinalLanguage</td>
            <td>@order.Status</td>
            <td>@order.OrderDate</td>
            <td>@order.OrderCost</td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = order.OrderId }) |
               
                @Html.ActionLink("Delete", "Delete", new { id = order.OrderId })
            </td>
        </tr>
    }
</table>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
